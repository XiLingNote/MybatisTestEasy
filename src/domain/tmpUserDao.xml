
    <!DOCTYPE mapper PUBLIC   
        "-//mybatis.org//DTD Mapper 3.0//EN"  
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
    <mapper namespace="tmpUserDao">  
        <select id="selectUser" parameterType="int" resultType="tmpUser">  
            select * from tmpUser where id = #{id}  
       </select>  
    </mapper>  